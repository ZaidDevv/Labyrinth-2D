<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="labyrinth.controllers.LeaderboardController">
   <children>
      <VBox alignment="CENTER" layoutX="20.0" layoutY="20.0" AnchorPane.bottomAnchor="20.0" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="20.0">
         <children>
            <TableView fx:id="tvLeaderboard" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="800.0" prefWidth="800.0">
              <columns>
                <TableColumn fx:id="name" maxWidth="-1.0" minWidth="160.0" prefWidth="-1.0" text="Nickname" />
                <TableColumn fx:id="startDate" maxWidth="-1.0" minWidth="180.0" prefWidth="-1.0" text="Start Date" />
                  <TableColumn fx:id="outcome" maxWidth="-1.0" minWidth="160.0" prefWidth="-1.0" text="Outcome" />
                  <TableColumn fx:id="score" maxWidth="-1.0" minWidth="160.0" prefWidth="-1.0" text="Score" />
                  <TableColumn fx:id="steps" maxWidth="-1.0" minWidth="160.0" prefWidth="-1.0" resizable="false" text="Steps" />
              </columns>
            </TableView>
            <JFXButton fx:id="homeBtn" alignment="CENTER" buttonType="RAISED" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" onAction="#handleHomeBtn" prefHeight="70.0" prefWidth="70.0" styleClass="button-raised" stylesheets="@../css/game.css" text="HOME" textAlignment="CENTER">
               <padding>
                  <Insets bottom="5.0" left="5.0" right="10.0" top="10.0" />
               </padding>
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin></JFXButton>
         </children>
         <padding>
            <Insets bottom="20.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
